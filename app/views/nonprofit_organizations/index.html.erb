<p id="notice"><%= notice %></p>

<h1>Listing Nonprofit Organizations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nonprofit_organizations.each do |nonprofit_organization| %>
      <tr>
        <td><%= link_to 'Show', nonprofit_organization %></td>
        <td><%= link_to 'Edit', edit_nonprofit_organization_path(nonprofit_organization) %></td>
        <td><%= link_to 'Destroy', nonprofit_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nonprofit organization', new_nonprofit_organization_path %>
